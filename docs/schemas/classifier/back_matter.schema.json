{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "back_matter.schema.json",
  "title": "Back Matter",
  "type": "object",
  "required": ["span", "text_sha256", "warnings"],
  "properties": {
    "span": {
      "type": "array",
      "items": { "type": "integer", "minimum": 0 },
      "minItems": 2,
      "maxItems": 2
    },
    "text_sha256": { "type": "string" },
    "warnings": { "type": "array", "items": { "type": "string" } }
  }
}
